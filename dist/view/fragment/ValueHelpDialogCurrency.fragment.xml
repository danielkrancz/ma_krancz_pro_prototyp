<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="Currency Code"
		ok=".onValueHelpOkPress"
		cancel=".onValueHelpCancelPress"
		afterClose=".onValueHelpAfterClose"
		supportRanges="{ui>/supportRanges}"
		supportMultiselect="{ui>/supportMultiselect}"
		key="Currency"
		descriptionKey="Currency_Text">
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearch" isRunningInValueHelpDialog="true">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="Currency"
						label="Currency"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Currency" />
						</fb:control>
					</fb:FilterGroupItem>

					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="Currency_Text"
						label="Description"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Currency_Text" />
						</fb:control>
					</fb:FilterGroupItem>

				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
